---
import Layout from '../layouts/Layout.astro'
import Title from '../components/Title.astro'
import Section from '../components/Section.astro'
import Seo from '../class/Seo'
import ReturnAction from '../class/ReturnAction'
import ButtonAction from '../class/ButtonAction'

const returnValues!: ReturnAction = new ReturnAction('/map', 'Plan du site')
const actions: ButtonAction[] = [
    new ButtonAction('/','Retour à l\'accueil'),
    new ButtonAction('/contact', 'Me contacter')
]
const seoData : Seo = new Seo('IBF Equicoaching | Plan du site', 'Plan du site et liens divers', false)
---
<Layout title="IBF Equicoaching | Plan du site" seoData={seoData}>
    <main>
        <Title title="Plan du site" returnValues={returnValues}/>
        <Section actions={actions}>
            <h2 slot="section-title">Pages de présentation</h2>
            <Fragment slot="section-content">
                <p>
                    <a href="/">Homepage</a><br>
                    <a href="/about">A propos</a><br>
                    <a href="/service">Prestations</a><br>
                    <a href="/company">Prestations pour entreprises</a><br>
                    <a href="/events">Evènements</a><br>
                    <a href="/pricing-equitation">Tarifs équitation</a><br>
                    <a href="/pricing-equicoaching">Tarifs équicoaching</a><br>
                    <a href="/bons">Chèques cadeaux</a><br>
                </p>
                <h3>Formations</h3>
                <p>
                    <a href="/blog/certification-qualiopi">Certification Qualiopi et IBF Equicoaching</a><br>
                    <a href="/pricing-training">Index de nos formations</a><br>
                    <a href="/program/manager-leadership">Formation - Posture Managériale et Leadership</a><br>
                    <a href="/program/cohesion-communication">Formation - Cohésion et Communication</a><br>
                    <a href="/program/soft-skills">Formation - Developper ses Soft Skills</a><br>
                </p>
                <h4>Me retrouver</h4>
                <p>
                    <a href="/equicoaching-grenoble">Grenoble et bassin Grenoblois</a><br>
                    <a href="/equicoaching-chambery">Chambéry et bassin Chambérien</a><br>
                    <a href="/equicoaching-aix-les-bains">Aix Les Bains</a><br>
                    <a href="/equicoaching-albertville">Albertville</a><br>
                    <a href="/equicoaching-annecy">Annecy</a><br>
                    <a href="/equicoaching-geneva">Geneva</a>/<a href="/equicoaching-geneve">Genève</a><br>
                    <a href="/equicoaching-switzerland">Switzerland</a>/<a href="/equicoaching-suisse">Suisse</a><br>
                </p>
                <h5>
                    Le blog / Faq
                </h5>
                <p>
                    <a href="/blog/index">Index</a><br>
                    <a href="/blog/certification-qualiopi">Qualiopi, qu'est ce que c'est ?</a><br>
                    <a href="/blog/etre-coach-26-ans">Etre coach à 26 ans, c'est possible ?</a><br>
                    <a href="/blog/deroulement-atelier-decouverte">Comment se déroule un atelier découverte collectif en
                        equicoaching ?</a><br>
                    <a href="/blog/deroulement-seminaire">Comment se déroule un séminaire / team building
                        Equicoaching&nbsp;?</a><br>
                    <a href="/blog/deroulement-seance-individuelle">Comment se déroule une séance d’equicoaching
                        individuelle&nbsp;?</a><br>
                    <a href="/blog/equicoaching-quest-ce-que-cest">L’equicoaching, qu’est-ce que c’est&nbsp;?</a><br>
                    <a href="/blog/intelligence-emotionnelle">L’intelligence émotionnelle et l'équicoaching</a><br>
                    <a href="/blog/objectifs-resolutions">Comment atteindre ses objectifs ?</a><br>
                    <a href="/blog/pourquoi-former-ses-managers">Pourquoi former ses managers grâce à l'equicoaching ?</a><br>
                </p>
                <h6>
                    Partenaires
                </h6>
                <p>
                    <a href="https://www.coachingrhe.fr/">Coaching RHE</a><br>
                    <a href="https://kareevents.fr/">KARE Events</a><br>
                </p>
                <h7>
                    Légal
                </h7>
                <p>
                    <a href="/contact">Contact</a><br>
                    <a href="/mentions-legales">Mentions légales</a><br>
                    <a href="/charte-accessibilite">Charte pour l'accueil en formation de personnes en situation de
                        handicap </a><br>
                    <a href="/cookies">Gestion des données personnelles</a><br>
                    <a href="/map">Plan du site</a>
                </p>
            </Fragment>
        </Section>
    </main>
</Layout>
